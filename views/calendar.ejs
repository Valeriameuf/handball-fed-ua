                                <img class="cell__img" src="@img/motor.png" alt="Клуб 4" />
                                Клуб 4
                            </div>
                        </div>
                        <div class="cell">
                            <div class="cell__content">2:1</div>
                        </div>
                        <div class="cell">
                            <a class="cell__link" href="#" target="_blank">
                                <img src="@img/youtube-color.svg" alt="youtube icon" />
                            </a>
                        </div>
                    </div>
                    <div class="row row--color">
                        <div class="cell">
                            <div class="cell__content">2025-02-22</div>
                        </div>
                        <div class="cell">
                            <div class="cell__content">19:30</div>
                        </div>
                        <div class="cell">
                            <div class="cell__content">
                                <img class="cell__img" src="@img/motor.png" alt="Клуб 5" />
                                Клуб 5 vs
                                <img class="cell__img" src="@img/motor.png" alt="Клуб 6" />
                                Клуб 6
                            </div>
                        </div>
                        <div class="cell">
                            <div class="cell__content">0:0</div>
                        </div>
                        <div class="cell">
                            <a class="cell__link" href="#" target="_blank">
                                <img src="@img/youtube-color.svg" alt="youtube icon" />
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
